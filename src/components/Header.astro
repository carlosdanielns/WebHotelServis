---
import HeaderButton from "./HeaderButton.astro";
---

<header>
 <nav
    class="pt-5 flex flex-col items-center justify-between w-full text-center xl:flex-row md:flex-col"
  >
    <img src="https://raw.githubusercontent.com/carlosdanielns/WebHotelServis/main/Img/logo.png
    " class="float-left" />
    <HeaderButton href="/" class="hover:bg-blue-700 active:bg-violet-700">
      <svg
        slot="before"
        width="24"
        height="24"
        viewBox="0 0 24 24"
        fill="none"
        stroke="currentColor"
        stroke-width="2"
        stroke-linecap="round"
        stroke-linejoin="round"
        class="icon icon-tabler icons-tabler-outline icon-tabler-home"
        ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
          d="M5 12l-2 0l9 -9l9 9l-2 0"></path><path
